#!/dis/sh -n
load std mpexpr

subfn isprime {
    list = ${expr 2 ${sqrt $1} seq}
    ok = 0
    for i in $list {
    	if {~ ${expr $1 $i '%'} 0} {
    		ok = 1
		raise break
    	}
    }
    if {~ $ok 1} {
    	result = 0
    } {
    	result = 1
    }
}

input = 2038074750 2038074751 2038074752 2038075231 2038075751 2038076267 2038074761 2038075271 2038075777 2038076281 2038074769 2038075289 2038075783 2038076303 2038074793 2038075301 2038075817 2038076311 2038074803 2038075307 2038075883 2038076317 2038074817 2038075339 2038075909 2038076323 2038074853 2038075349 2038075913 2038076329 2038074923 2038075363 2038075933 2038076351 2038074931 2038075373 2038075943 2038076357 2038074947 2038075387 2038075967 2038076423 2038074949 2038075397 2038076003 2038076449 2038075001 2038075423 2038076057 2038076473 2038075019 2038075433 2038076083 2038076479 2038075037 2038075463 2038076099 2038076489 2038075049 2038075469 2038076101 2038076507 2038075069 2038075511 2038076111 2038076539 2038075099 2038075531 2038076113 2038076543 2038075121 2038075547 2038076119 2038076627 2038075163 2038075579 2038076129 2038076639 2038075183 2038075597 2038076137 2038076653 2038075187 2038075619 2038076171 2038076717 2038075189 2038075639 2038076191 2038076723 2038075199 2038075643 2038076213 2038076737 2038075219 2038075649 2038076221 2038076767 2038075229 2038075691 2038076233 2038076783

ans = 0

for i in $input {
    if {~ ${isprime $i} 1}
       ans = ${expr $ans 1 +}
}

echo "There is $ans primes in the input list"
